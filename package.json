{
  "name": "@ska-telescope/ska-gui-components",
  "version": "1.0.26",
  "description": "A Library of SKA GUI components using Material-UI",
  "author": "Trevor A Swain",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "browserList": "npx browserslist@latest --update-db",
    "build": "react-scripts build",
    "build-storybook": "storybook build",
    "code-analysis": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:open": "cypress open",
    "cypress:component:chrome": "cypress run --component --browser chrome --config video=false",
    "lint": "eslint --ignore-path .eslintignore",
    "lint:fix": "eslint  --ignore-path .eslintignore --fix",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\"",
    "prettier:fix": "prettier src/**/*.{js,jsx,ts,tsx} --write",
    "rollup": "rollup --bundleConfigAsCjs -c",
    "skao:update": "yarn add @ska-telescope/ska-javascript-components@latest",
    "storybook": "storybook dev -p 6006",
    "storybook:update": "npx storybook@latest upgrade",
    "test:component:headless": "cypress run --component --headless --browser chrome --config video=false",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports",
    "test:cypress:component:ci": "cypress run --component --headless --browser chrome --reporter junit --config video=false --reporter-options mochaFile=build/tests/unit-tests[hash].xml"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.jsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "dependencies": {
    "@mui/icons-material": "^5.15.19",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@mui/system": "^5.15.15",
    "@mui/x-data-grid": "^7.3.0",
    "@mui/x-tree-view": "6.17.0",
    "@react-spring/web": "^9.7.3",
    "@ska-telescope/ska-javascript-components": "^1.0.9",
    "@types/uuid": "^9.0.4",
    "junit-report-merger": "^6.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-flow": "^7.24.6",
    "@babel/plugin-transform-react-jsx": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@cypress/code-coverage": "^3.12.39",
    "@cypress/instrument-cra": "^1.4.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.5",
    "@fontsource/material-icons": "^5.0.18",
    "@fontsource/roboto": "^5.0.13",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^7.6.13",
    "@storybook/addon-interactions": "^7.6.13",
    "@storybook/addon-links": "^7.6.13",
    "@storybook/node-logger": "^7.6.13",
    "@storybook/preset-create-react-app": "^7.6.13",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.6.13",
    "@storybook/react-webpack5": "^7.6.13",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.19",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-preset-react-app": "^10.0.1",
    "css-loader": "^7.1.2",
    "cypress": "^13.10.0",
    "debug": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "json5": "^2.2.3",
    "junit": "^1.4.9",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-refresh": "^0.14.2",
    "react-scripts": "^5.0.0",
    "require-from-string": "^2.0.2",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "sass-loader": "^14.2.1",
    "storybook": "^7.6.13",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
