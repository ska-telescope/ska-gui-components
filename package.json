    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "rollup": "rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettify": "prettier src/**/*.{ts,tsx} --write"
  },
  "devDependencies": {
