  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(i18n),}.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.jsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
