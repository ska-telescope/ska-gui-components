{
  "name": "@ska-telescope/ska-gui-components",
  "version": "0.1.19",
  "description": "A Library of SKA GUI components using Material-UI",
  "author": "Trevor A Swain",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "browserList": "npx browserslist@latest --update-db",
    "build": "webpack --mode production",
    "build-storybook": "build-storybook",
    "code-analysis": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run --component",
    "cypress:component:chrome": "cypress run --component --browser chrome",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "yarn tsc --noEmit",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "start": "NODE_ENV=test webpack serve --mode development --open",
    "dev": "webpack serve --open --mode development",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\"",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "rollup": "rollup -c",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "test": "yarn cypress:open",
    "test:component:headless": "cypress run --component --headless --browser chrome",
    "test:e2e:headless": "cypress run --e2e --headless --browser chrome",
    "test:headless": "yarn cypress:run",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports --reporter=cobertura",
    "test:coverage:summary": "npx nyc report --reporter=text-summary",
    "test:cypress:ci": "cypress run --e2e --headless --browser chrome --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^7.0.23",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^7.0.2",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.16",
    "@types/react": "^17.0.34",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.7.3",
    "cypress": "^12.8.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "junit": "^1.4.9",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-scripts": "^5.0.0",
    "require-from-string": "^2.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass-loader": "^13.2.2",
    "storybook-dark-mode": "^3.0.0",
    "style-loader": "^3.3.2",
    "typescript": "^4.4.4",
    "webpack": "^5.77.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(i18n),}.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.jsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@ska-telescope/ska-gui-components": "^0.1.16",
    "@ska-telescope/ska-javascript-components": "^0.1.21",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "moment": "^2.29.4",
    "react-i18next": "^12.2.0"
  }
}
