{
  "name": "@ska-telescope/ska-gui-components",
  "version": "0.1.31",
  "description": "A Library of SKA GUI components using Material-UI",
  "author": "Trevor A Swain",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "browserList": "npx browserslist@latest --update-db",
    "build": "react-scripts build",
    "cypress": "cypress open",
    "cypress:component:chrome": "cypress run --component --browser chrome --config video=false",
    "code-analysis": "eslint --ignore-path .eslintignore -f junit -o linting.xml",
    "lint:fix": "eslint  --ignore-path .eslintignore --fix",
    "lint": "eslint --ignore-path .eslintignore",
    "prettier:fix": "prettier src/**/*.{js,jsx,ts,tsx} --write",
    "rollup": "rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:component:headless": "cypress run --component --headless --browser chrome --config video=false",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports --reporter=cobertura",
    "test:cypress:component:ci": "cypress run --component --headless --browser chrome --reporter junit --config video=false --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\""
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.22.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^7.0.2",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.16",
    "@types/react": "^17.0.34",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-istanbul": "^6.1.1",
    "css-loader": "^6.7.3",
    "cypress": "^12.8.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "junit": "^1.4.9",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-scripts": "^5.0.0",
    "require-from-string": "^2.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass-loader": "^13.2.2",
    "storybook": "^7.0.24",
    "style-loader": "^3.3.2",
    "typescript": "^4.4.4",
    "webpack": "^5.77.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.jsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@mui/lab": "^5.0.0-alpha.135",
    "@mui/system": "^5.13.7",
    "@mui/x-data-grid": "^6.9.2",
    "@react-spring/web": "^9.7.3",
    "@ska-telescope/ska-javascript-components": "^0.1.21",
    "junit-report-merger": "^6.0.2"
  }
}
